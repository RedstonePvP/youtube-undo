{
    "name": "Yotube Undo",
    "action": {},
    "manifest_version": 3,
    "version": "1.0",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "description": "Undo your last missclick on a youtube video",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["injectScript.js"]
        }
    ]
}